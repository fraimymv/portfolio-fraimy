		{
			"name": "Mail and Phone#",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let jsonData = pm.response.json();\r",
							"\r",
							"jsonData.forEach((contact, index) => {\r",
							"    const email = contact.email || \"sin email\";\r",
							"    const phone = contact.phone || \"sin teléfono\";\r",
							"    const contactoID = contact._id || `Contacto #${index + 1}`;\r",
							"\r",
							"    // Validar correo\r",
							"    const emailValido = /@gmail\\.com$|@mail\\.com$|@hotmail\\.com|@outlook\\.com/.test(email);\r",
							"    pm.test(`${contactoID} - Validar email (${email})`, function () {\r",
							"        pm.expect(emailValido, `❌ Email inválido: ${email} → Debe terminar en @gmail.com o @mail.com`).to.be.true;\r",
							"    });\r",
							"\r",
							"    // Validar teléfono\r",
							"    const telefonoValido = /^8/.test(phone);\r",
							"    pm.test(`${contactoID} - Validar teléfono (${phone})`, function () {\r",
							"        pm.expect(telefonoValido, `❌ Teléfono inválido: ${phone} → Debe comenzar con 8`).to.be.true;\r",
							"    });\r",
							"});"
						],
