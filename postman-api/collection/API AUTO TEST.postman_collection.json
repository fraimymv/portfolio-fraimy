				{
					"listen": "test",
					"script": {
						"exec": [
							"// Guarda el JSON de la respuesta\r",
							"let jsonData = pm.response.json();\r",
							"\r",
							"// Verifica que haya al menos un contacto\r",
							"pm.test(\"Hay al menos un contacto\", function () {\r",
							"    pm.expect(jsonData.length).to.be.above(0);\r",
							"});\r",
							"\r",
							"// Verifica que todos los correos tienen un @\r",
							"jsonData.forEach((contact, index) => {\r",
							"    const id = contact._id || \"sin ID\";\r",
							"    const email = contact.email || \"sin email\";\r",
							"\r",
							"    pm.test(`Contacto ${index + 1} (${email}) - Validar nombre`, function () {\r",
							"        pm.expect(contact.firstName).to.match(\r",
							"            /^[A-Z][a-z]*$/,\r",
							"            `Nombre incorrecto: ${contact.firstName} (ID: ${id})`\r",
							"        );\r",
							"    });\r",
							"\r",
							"    pm.test(`Contacto ${index + 1} (${email}) - Validar apellido`, function () {\r",
							"        pm.expect(contact.lastName).to.match(\r",
							"            /^[A-Z][a-z]*$/,\r",
							"            `Apellido incorrecto: ${contact.lastName} (ID: ${id})`\r",
							"        );\r",
							"    });\r",
							"});\r",
							"\r",
							"\r",
							"//Verificar nombre y apellido\r",
							"pm.test(\"Todos los nombres y apellidos están correctamente capitalizados\", function () {\r",
							"    jsonData.forEach(contact => {\r",
							"        // Verificar el nombre\r",
							"        pm.expect(contact.firstName).to.match(/^[A-Z][a-z]*$/, `Nombre incorrecto: ${contact.firstName}`);\r",
							"\r",
							"        // Verificar el apellido\r",
							"        pm.expect(contact.lastName).to.match(/^[A-Z][a-z]*$/, `Apellido incorrecto: ${contact.lastName}`);\r",
							"    });\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
